{
  "version": "2.0",
  "extensions": {
    "durableTask": {
      "hubName": "%ProcessManagerTaskHubName%",
      "storageProvider": {
        "connectionName": "ProcessManagerStorageConnectionString",
        "partitionCount": 16
      },
      "maxConcurrentOrchestratorFunctions": 15
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  }
}
